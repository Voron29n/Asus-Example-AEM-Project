<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Hero Banner Small"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[asus.components.hero-banner.banner-small.v1.dialog]">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">

            <tabs jcr:primaryType="nt:unstructured"
                  maximized="{Boolean}true"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <heroImageTab
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/hero-banner/hero-banner-small/v1/hero-banner-small/cq:dialog/tabs/heroImageTab"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />

                    <heroTextTab
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/hero-banner/hero-banner-small/v1/hero-banner-small/cq:dialog/tabs/heroTextTab"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />

                    <heroLinkTab
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/hero-banner/hero-banner-small/v1/hero-banner-small/cq:dialog/tabs/heroLinkTab"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />

                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
