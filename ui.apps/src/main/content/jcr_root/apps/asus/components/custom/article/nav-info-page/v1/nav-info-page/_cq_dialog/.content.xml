<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Navigation info page v1"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[asus.components.nav-info-page.v1.dialog]">
    <content
            granite:class="cmp-nav-info-page_v1__editor"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">

            <tabs jcr:primaryType="nt:unstructured"
                  maximized="{Boolean}true"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <includeTitle
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/nav-info-page/v1/nav-info-page/cq:dialog/tabs/title"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />
                    <includeNavGroups
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/nav-info-page/v1/nav-info-page/cq:dialog/tabs/navGroups"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />
                </items>
            </tabs>
        </items>
    </content>

</jcr:root>
