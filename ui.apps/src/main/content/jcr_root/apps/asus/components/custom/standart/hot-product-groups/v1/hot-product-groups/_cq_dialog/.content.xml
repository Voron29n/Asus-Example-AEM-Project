<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Hot product groups v1"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[asus.components.hot-product-groups.v1.dialog]">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container"
            granite:class="cmp-hot_product-groups_v1__editor">
        <items jcr:primaryType="nt:unstructured">

            <tabs jcr:primaryType="nt:unstructured"
                  maximized="{Boolean}true"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <includeTitleProductGroups
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/standart/hot-product-groups/v1/hot-product-groups/cq:dialog/tabs/title"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />
                    <includeProductGroups
                            jcr:primaryType="nt:unstructured"
                            path="/apps/asus/components/custom/standart/hot-product-groups/v1/hot-product-groups/cq:dialog/tabs/productGroups"
                            sling:resourceType="granite/ui/components/foundation/include"
                    />
                </items>
            </tabs>
        </items>
    </content>

</jcr:root>
