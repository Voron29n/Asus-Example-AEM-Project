<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/asus/settings/wcm/templates/page-content-ProArt-Professional"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="asus/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="asus/components/custom/parsys/layout-container">
                    <prof-hero-img
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="asus/components/custom/proart/professional/hero-img/v1/hero-img"
                            editable="{Boolean}true"/>
                    <prof-person-info
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="asus/components/custom/proart/professional/person-info/v1/person-info"
                            editable="{Boolean}true"/>
                    <prof-person-text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="asus/components/custom/proart/professional/person-text/v1/person-text"
                            editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="2"
                                    width="8"/>
                            <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            <tablet
                                    jcr:primaryType="nt:unstructured"
                                    offset="1"
                                    width="10"/>
                        </cq:responsive>
                    </prof-person-text>
                </container>
        </root>
    </jcr:content>
</jcr:root>
