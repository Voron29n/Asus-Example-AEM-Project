<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:deviceGroups="[/etc/mobile/groups/responsive]"
            cq:template="/conf/asus/settings/wcm/templates/page-content-ProArt-Professional"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="asus/components/page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="asus/components/container"
                layout="simple">

            <experiencefragment-header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="asus/components/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/asus/ru/ru/site/header/master"/>

            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="asus/components/custom/parsys/layout-container"
                    editable="{Boolean}true"/>

            <experiencefragment-footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="asus/components/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/asus/ru/ru/site/footer/master_v3"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                        jcr:primaryType="nt:unstructured"
                        title="Extra Small"
                        width="{Long}480"/>
                <tablet
                        jcr:primaryType="nt:unstructured"
                        title="Medium Screen"
                        width="{Long}768"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
